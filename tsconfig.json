{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@navigator": [
        "./src/navigator/index.ts"
      ],
      "@navigator/*": [
        "./src/navigator/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@theme": [
        "./src/theme/index.ts"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ]
    }
  }
}