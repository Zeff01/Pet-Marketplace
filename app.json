{
  "expo": {
    "name": "pet-marketplace",
    "slug": "pet-marketplace",
    "privacy": "unlisted",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/pet-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pet-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-sm.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/pet-icon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}